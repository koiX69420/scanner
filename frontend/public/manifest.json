{
    "manifest_version": 3,
    "name": "Solana Token Analyzer",
    "version": "1.0",
    "description": "Analyze Solana tokens instantly!",
    "permissions": ["storage"],
    "host_permissions": ["*://*/*"], 
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "32": "MD.jpg"
      }
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'none';"
    }
  }
  